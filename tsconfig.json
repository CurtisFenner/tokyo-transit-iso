{
	"compilerOptions": {
		"strict": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"sourceMap": true,
		"skipLibCheck": true,
		/** Emitted by Vite; use `yarn run build`. */
		"noEmit": true,
		"lib": [
			"ES2020",
			"DOM",
			"DOM.Iterable"
		]
	},
	"include": [
		"src/**/*.ts",
	]
}
