{
	"compilerOptions": {
		"strict": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"sourceMap": true,
		"skipLibCheck": true,
		/** Emitted by Vite; use `yarn run build`. */
		"noEmit": true,
		"lib": [
			"ES2020",
			"DOM",
			"DOM.Iterable"
		],
		"jsx": "react-jsx",
	},
	"include": [
		"src/*.ts",
		"src/**/*.ts",
		"src/**/*.tsx",
		"src/search.ts"
	]
}
